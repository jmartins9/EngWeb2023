extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 EMDs List
    .w3-container
      table.w3-table-all
        tr
          th Data
          th Nome
          th Género
          th Idade 
          th Resultado
  
        each e in list
          tr
            td= e.dataEMD
            td= e.nome
            td= e.género
            td= e.idade 
            td= e.resultado
            
    footer.w3-container.w3-teal
      h5 Generated in EW
    